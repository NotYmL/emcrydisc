{
    "name":"encrydisc",
	"description":"encryptor",
    "version": "1.0",
    "manifest_version":2,
	"permissions": [
		"storage"
	],
	"content_scripts": [
		{
			"matches": [
				"https://discord.com/channels/*",
				"https://ptb.discord.com/channels/*",
				"https://canary.discord.com/channels/*"
			],
			"all_frames": true,
			"js": ["discord.js"],
			"run_at": "document_start"
		}
	]
}
